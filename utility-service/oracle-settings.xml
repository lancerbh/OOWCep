	<pluginGroups>
		<pluginGroup>com.oracle.maven</pluginGroup>
	</pluginGroups>

	<servers>

		<server>
			<id>maven.oracle.com</id>
			<username>llcastro@grupomult.com.br</username>
			<password>mO300341@</password>
			<configuration>
				<basicAuthScope>
					<host>ANY</host>
					<port>ANY</port>
					<realm>OAM 11g</realm>
				</basicAuthScope>
				<httpConfiguration>
					<all>
						<params>
							<property>
								<name>http.protocol.allow-circular-redirects</name>
								<value>%b,true</value>
							</property>
						</params>
					</all>
				</httpConfiguration>
			</configuration>
		</server>

	</servers>

	<profiles>

		<profile>

			<id>main</id>

			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>

			<repositories>
				<repository>
					<id>maven.oracle.com</id>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
					</releases>
					<snapshots>
						<enabled>false</enabled>
						<updatePolicy>never</updatePolicy>
					</snapshots>
					<url>https://maven.oracle.com</url>
					<layout>default</layout>
				</repository>
			</repositories>

			<pluginRepositories>
				<pluginRepository>
					<id>maven.oracle.com</id>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
					</releases>
					<snapshots>
						<enabled>false</enabled>
						<updatePolicy>never</updatePolicy>
					</snapshots>
					<url>https://maven.oracle.com</url>
					<layout>default</layout>
				</pluginRepository>
			</pluginRepositories>

		</profile>

	</profiles>

	<activeProfiles>
		<activeProfile>main</activeProfile>
	</activeProfiles>

</settings>
