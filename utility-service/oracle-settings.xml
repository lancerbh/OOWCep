<settings xmlns="http://maven.apache.org/SETTINGS/1.1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">

	<pluginGroups>
		<pluginGroup>com.oracle.maven</pluginGroup>
	</pluginGroups>

	<servers>
		<server>
			<id>maven.oracle.com</id>
			<username>llcastro@grupomult.com.br</username>
			<password>mO300341@</password>
			<configuration>
				<basicAuthScope>
					<host>ANY</host>
					<port>ANY</port>
					<realm>OAM 11g</realm>
				</basicAuthScope>
				<httpConfiguration>
					<all>
						<params>
							<property>
								<name>http.protocol.allow-circular-redirects</name>
								<value>%b,true</value>
							</property>
						</params>
					</all>
				</httpConfiguration>
			</configuration>
		</server>
	</servers>
	<profiles>
		<profile>
			<id>main</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<pluginRepositories>

				<pluginRepository>
					<id>maven.oracle.com</id>
					<url>https://maven.oracle.com</url>
					<snapshots>
						<enabled>true</enabled>
						<updatePolicy>always</updatePolicy>
						<updatePolicy>interval:86400</updatePolicy>
					</snapshots>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>always</updatePolicy>
						<updatePolicy>interval:86400</updatePolicy>
					</releases>
				</pluginRepository>
			</pluginRepositories>

			<repositories>
				<repository>
					<id>maven.oracle.com</id>
					<url>https://maven.oracle.com</url>
					<snapshots>
						<enabled>true</enabled>
						<updatePolicy>always</updatePolicy>
						<updatePolicy>interval:86400</updatePolicy>
					</snapshots>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>always</updatePolicy>
						<updatePolicy>interval:86400</updatePolicy>
					</releases>
				</repository>
			</repositories>
		</profile>
	</profiles>

	<activeProfiles>
		<activeProfile>main</activeProfile>
	</activeProfiles>

</settings>
